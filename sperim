#!/usr/bin/python

import modSperim

#mrmr 
#PageRank                      
#InfoGainAttributeEval	         
#ReliefFAttributeEval	         
#ChiSquaredAttributeEval	
#GainRatioAttributeEval 
#PCA	                    
#SVMAttributeEval          

cmdThreadFS="ps aux | grep \'java -cp lodrecsys.jar di.uniba.it.lodrecsys.graph.GraphFSRun\' | grep -v \'color=auto\' | grep -v \'/bin/sh\' |wc -l"

cmdThreadRec="ps aux | grep \'java -cp lodrecsys.jar di.uniba.it.lodrecsys.graph.GraphRecRun\' | grep -v \'color=auto\' | grep -v \'/bin/sh\' |wc -l"

cmdThreadEval="ps aux | grep \'java -cp lodrecsys.jar di.uniba.it.lodrecsys.graph.GraphEvalRun\' | grep -v \'color=auto\' | grep -v \'/bin/sh\' |wc -l"

param=8
allalg=["PageRank","MRMR"]
#allalgWEKA=["ChiSquaredAttributeEval"] 
#allalg=["PageRank","MRMR"]
allalgWEKA=["ChiSquaredAttributeEval","GainRatioAttributeEval","SVMAttributeEval", "InfoGainAttributeEval","SVMAttributeEval","PCA","ReliefFAttributeEval"]
topN=["10","17","30","50"]
givenN=["given_5","given_10","given_20","given_30","given_50","given_all"]
metrics=["F1","Diversity"]
modSperim.sperim(allalg, allalgWEKA, topN, givenN, param, cmdThreadFS, cmdThreadRec, cmdThreadEval, metrics)
